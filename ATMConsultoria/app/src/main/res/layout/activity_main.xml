<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/layoutId"
        android:layout_width="368dp"
        android:layout_height="576dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/logoId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="70dp"
            app:srcCompat="@drawable/logo" />

        <ImageView
            android:id="@+id/clientesId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/logoId"
            android:layout_alignTop="@+id/contatoId"
            app:srcCompat="@drawable/menu_cliente" />

        <ImageView
            android:id="@+id/contatoId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/logoId"
            android:layout_alignParentTop="true"
            android:layout_marginTop="208dp"
            app:srcCompat="@drawable/menu_contato" />

        <ImageView
            android:id="@+id/empresaId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/logoId"
            android:layout_marginBottom="97dp"
            app:srcCompat="@drawable/menu_empresa" />

        <ImageView
            android:id="@+id/servicosId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/logoId"
            android:layout_alignTop="@+id/empresaId"
            app:srcCompat="@drawable/menu_servico" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>